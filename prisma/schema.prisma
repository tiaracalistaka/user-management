datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  password  String
  role      Role     @default(PEGAWAI)
  createdAt DateTime @default(now())
}

enum Role {
  SUPERADMIN
  MANAGER
  PEGAWAI
}

